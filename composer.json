{
    "name": "aerni/dynamic-cache",
    "description": "Dynamically exclude URLs from the static cache",
    "keywords": [
        "statamic",
        "static cache"
    ],
    "homepage": "https://github.com/aerni/statamic-dynamic-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Aerni",
            "email": "hello@michaelaerni.ch",
            "homepage": "https://www.michaelaerni.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "statamic/cms": "^3.0",
        "stillat/proteus": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Aerni\\DynamicCache\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Dynamic Cache",
            "description": "Dynamically exclude URLs from the static cache"
        },
        "laravel": {
            "providers": [
                "Aerni\\DynamicCache\\ServiceProvider"
            ]
        }
    }
}
